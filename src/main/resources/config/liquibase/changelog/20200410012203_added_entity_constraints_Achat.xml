<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Achat.
    -->
    <changeSet id="20200410012203-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="audit_id"
                                 baseTableName="achat"
                                 constraintName="fk_achat_audit_id"
                                 referencedColumnNames="id"
                                 referencedTableName="audit"/>

        <addForeignKeyConstraint baseColumnNames="acheteur_id"
                                 baseTableName="achat"
                                 constraintName="fk_achat_acheteur_id"
                                 referencedColumnNames="id"
                                 referencedTableName="utilisateur"/>

        <addForeignKeyConstraint baseColumnNames="article_id"
                                 baseTableName="achat"
                                 constraintName="fk_achat_article_id"
                                 referencedColumnNames="id"
                                 referencedTableName="article"/>

        <addForeignKeyConstraint baseColumnNames="entreprise_id"
                                 baseTableName="achat"
                                 constraintName="fk_achat_entreprise_id"
                                 referencedColumnNames="id"
                                 referencedTableName="entreprise"/>

    </changeSet>
</databaseChangeLog>
