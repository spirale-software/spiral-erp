<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Vente.
    -->
    <changeSet id="20200421173712-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="audit_id"
                                 baseTableName="vente"
                                 constraintName="fk_vente_audit_id"
                                 referencedColumnNames="id"
                                 referencedTableName="audit"/>

        <addForeignKeyConstraint baseColumnNames="vendeur_id"
                                 baseTableName="vente"
                                 constraintName="fk_vente_vendeur_id"
                                 referencedColumnNames="id"
                                 referencedTableName="utilisateur"/>

        <addForeignKeyConstraint baseColumnNames="article_id"
                                 baseTableName="vente"
                                 constraintName="fk_vente_article_id"
                                 referencedColumnNames="id"
                                 referencedTableName="article"/>

        <addForeignKeyConstraint baseColumnNames="entreprise_id"
                                 baseTableName="vente"
                                 constraintName="fk_vente_entreprise_id"
                                 referencedColumnNames="id"
                                 referencedTableName="entreprise"/>

    </changeSet>
</databaseChangeLog>
